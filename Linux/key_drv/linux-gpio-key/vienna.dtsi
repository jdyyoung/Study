			spinand_7: spinand@7 {
				vatics,nand-name = "HeYang HY5F1GQ4AACAE 1G 3.3V SPI";
				vatics,nand-info = <0xc9510000 0x0 0x800 0x80 0x20000 0x0 0x2 0x80 0x0 0x0 0x0>;
